{
    "name": "vendor_slug/package_slug",
    "description": ":package_description",
    "keywords": [
        ":vendor_name",
        "laravel",
        "package_slug"
    ],
    "homepage": "https://github.com/vendor_slug/package_slug",
    "license": "MIT",
    "authors": [
        {
            "name": ":author_name",
            "email": "author@domain.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/contracts": "^8.0",
        "lorisleiva/laravel-actions": "^2.1",
        "nikservik/admin-dashboard": "^1.0",
        "nikservik/commons": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.15",
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "VendorName\\Skeleton\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VendorName\\Skeleton\\Tests\\": "tests",
            "App\\": "laravel/app/",
            "Database\\Factories\\": "laravel/database/factories/"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "github-oauth": {
            "github.com": "--- insert token ---"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "VendorName\\Skeleton\\SkeletonServiceProvider"
            ]
        }
    },
    "prefer-stable": true,
    "repositories" : [
        {
            "type": "vcs",
            "url" : "git@github.com:nikservik/admin-dashboard"
        },
        {
            "type": "vcs",
            "url" : "git@github.com:nikservik/commons"
        },
        {
            "type": "vcs",
            "url" : "git@github.com:nikservik/user"
        }
    ]
}
